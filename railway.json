{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "providers": ["...", "python"],
      "phases": {
        "setup": {
          "nixPkgs": ["...", "google-chrome", "chromedriver"]
        }
      }
    }
  },
    "deploy": {
        "startCommand": "uvicorn server:app --host 0.0.0.0 --port $PORT"
    }
}
